package dut.udn.watchshop.entity;

import javax.persistence.Entity;

import lombok.Data;

@Entity
@Data
public class Watch extends Product{
	private Brand brand;
	private boolean sex;
	private String color;
	private float size;
	private String albert;
	public static void main(String[] args) {
		Watch obj = new Watch();
		obj.price = 1;
	}
}
